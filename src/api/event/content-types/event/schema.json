{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 40,
      "unique": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Concert",
        "Konzert",
        "Festival",
        "Kufa",
        "Treff",
        "Andere"
      ],
      "default": "Concert",
      "required": true
    },
    "eventtext": {
      "type": "richtext"
    },
    "eventstart": {
      "type": "datetime",
      "required": true,
      "default": "2024-06-17T18:00:28.495Z"
    },
    "eventend": {
      "type": "datetime"
    },
    "bands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::band.band",
      "inversedBy": "events"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "ticket": {
      "displayName": "ticket",
      "type": "component",
      "repeatable": false,
      "component": "ticket.ticket",
      "required": true
    },
    "creator": {
      "type": "string"
    },
    "flyer": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
